{
  "name": "gatsby-source-newsapi",
  "version": "0.0.3",
  "description": "Gatsby source plugin for building websites using NewsAPI as a data source",
  "main": "index.js",
  "scripts": {
    "test": "jasmine",
    "build": "babel src --out-dir . --ignore __tests__",
    "prepublish": "cross-env NODE_ENV=production npm run build",
    "watch": "babel -w src --out-dir . --ignore __tests__"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thomann061/gatsby-source-newsapi.git"
  },
  "keywords": [
    "gatsby",
    "gatsby-source",
    "gatsby-source-newsapi"
  ],
  "author": "Jacob Thomann",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/thomann061/gatsby-source-newsapi/issues"
  },
  "homepage": "https://github.com/thomann061/gatsby-source-newsapi#readme",
  "dependencies": {
    "axios": "^0.18.0",
    "babel-runtime": "^6.26.0",
    "crypto": "^1.0.1",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-3": "^6.24.1",
    "cross-env": "^5.1.3",
    "jasmine": "^3.1.0"
  },
  "babel": {
    "presets": [
      "env",
      "stage-3"
    ]
  }
